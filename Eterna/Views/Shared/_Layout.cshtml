    <script src="/Content/js/google-code-prettify/prettify.js"></script>
    <script src="/Content/js/jquery.bxslider.min.js"></script>
    <script src="/Content/js/camera/camera.js"></script>
    <script src="/Content/js/camera/setting.js"></script>

    <script src="/Content/js/jquery.prettyPhoto.js"></script>
    <script src="/Content/js/portfolio/jquery.quicksand.js"></script>
    <script src="/Content/js/portfolio/setting.js"></script>

    <script src="/Content/js/jquery.flexslider.js"></script>
    <script src="/Content/js/animate.js"></script>
    <script src="/Content/js/inview.js"></script>

   
    <script src="/Content/js/custom.js"></script>

